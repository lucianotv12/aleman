

RewriteEngine on

#RewriteCond %{SERVER_PORT} 80

#RewriteRule ^(.*)$ https://www.myoriginalworld.es/$1 [R,L]

RewriteCond %{SCRIPT_FILENAME} !-d
RewriteCond %{SCRIPT_FILENAME} !-f

RewriteRule ^(.+)\.html ctrl/index.php?accion=$1 

RewriteRule ^([a-zA-Z0-9_-]+)/(.*)/$ ctrl/index.php?accion=$1&id=$2

RewriteRule ^([a-zA-Z0-9_-]+)-(.*)-(.*)$ ctrl/index.php?accion=$1&id=$2&tipo=$3

Options -Indexes

#order deny,allow
#deny from all
#allow from 181.47.212.19
